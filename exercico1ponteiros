#include <stdio.h>

int main() {
    int num1, num2;
    int *ptr1, *ptr2;
    printf("Digite o primeiro número: ");
    scanf("%d", &num1);
    printf("Digite o segundo número: ");
    scanf("%d", &num2); 

    ptr1 = &num1;
    ptr2 = &num2;

    if (ptr1 > ptr2) {
        printf("Conteúdo do maior endereço: %d\n", num1);
    } else if (ptr1 < ptr2) {
        printf("Conteúdo do maior endereço: %d\n", num2);
    } else {
        printf("Os endereços são iguais.\n");
    }

    return 0;
}
